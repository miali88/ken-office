{
  "name": "kenneth-office-addin-unified",
  "version": "1.0.0",
  "description": "Unified Office Add-in for Word and Outlook - Kenneth AI",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenneth-ai/office-agent.git"
  },
  "license": "MIT",
  "config": {
    "dev_server_port": 3000
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "dev-server": "webpack serve --mode development",
    "dev": "npm run dev-server",
    "lint": "office-addin-lint check",
    "lint:fix": "office-addin-lint fix",
    "prettier": "office-addin-lint prettier",
    "signin": "office-addin-dev-settings m365-account login",
    "signout": "office-addin-dev-settings m365-account logout",
    "start:word": "office-addin-debugging start manifests/manifest-word.xml",
    "start:outlook": "office-addin-debugging start manifests/manifest-outlook.xml",
    "stop": "office-addin-debugging stop",
    "validate:word": "office-addin-manifest validate manifests/manifest-word.xml",
    "validate:outlook": "office-addin-manifest validate manifests/manifest-outlook.xml",
    "watch": "webpack --mode development --watch"
  },
